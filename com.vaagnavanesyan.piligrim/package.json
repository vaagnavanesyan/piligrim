{
  "name": "piligrim",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "connect-android": "dotenv-load adb connect %android_ip%",
    "clean-android": "cd android && ./gradlew clean",
    "start": "react-native start",
    "start-android": "concurrently \"yarn start\" \"yarn android\"",
    "start-android-release": "react-native run-android --variant release",
    "start-ios": "concurrently \"yarn start\" \"yarn ios\"",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.6",
    "@react-navigation/native": "5.0.8",
    "@react-navigation/stack": "5.1.0",
    "dotenv": "^8.2.0",
    "dotenv-load": "^2.0.0",
    "native-base": "^2.13.12",
    "node-libs-react-native": "^1.2.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-cheerio": "^1.0.0-rc.4",
    "react-native-gesture-handler": "1.6.0",
    "react-native-reanimated": "1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "2.2.0",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-url-polyfill": "^1.1.2",
    "scrape-it": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/eslint-config": "0.0.7",
    "@types/jest": "24.9.1",
    "@types/react-native": "0.61.17",
    "@types/react-native-snap-carousel": "^3.8.1",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "2.12.0",
    "@typescript-eslint/parser": "2.12.0",
    "babel-jest": "24.9.0",
    "concurrently": "5.1.0",
    "eslint": "6.5.1",
    "jest": "25.3.0",
    "metro-react-native-babel-preset": "0.56.0",
    "react-test-renderer": "16.9.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
